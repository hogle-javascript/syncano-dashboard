  "scripts": {
    "test": "export NODE_ENV=testing && karma start karma.config.js",
    "start": "gulp webpack-dev-server",
    "build": "gulp build",
    "publish": "gulp publish",
    "clean": "gulp clean",
    "oauth-server": "node oauth-server.js",
    "lint": "eslint src --quiet --ext .js --ext .jsx",
    "e2e": "nightwatch -e chrome --suiteRetries 1",
    "e2e-0": "nightwatch -e chrome -g instances --suiteRetries 1",
    "e2e-1": "nightwatch -e chrome -s instances --suiteRetries 1",
    "e2e-2": "nightwatch -e firefox --suiteRetries 1 --skiptags socialLogins,instanceSettingsNav,schedules,authSettings,templates,templateView,pushSockets,scriptSockets",
    "e2e-local": "nightwatch -e chrome --suiteRetries 1 --skiptags socialLogins",
    "e2e-local-ff": "nightwatch -e firefox --suiteRetries 1 --skiptags socialLogins,instanceSettingsNav,schedules,authSettings,templates,templateView,pushSockets,scriptSockets",
    "e2e-screenshots": "nightwatch -e chrome,360x640,768x1024 --tag navigation || true",
    "e2e-setup": "gulp selenium-install",
    "e2e-http-server": "http-server ./dist_e2e --ssl --cert ./node_modules/webpack-dev-server/ssl/server.crt --key ./node_modules/webpack-dev-server/ssl/server.key",
    "iconfont": "gulp iconfont",
    "add-github-tag": "gulp add-github-tag",
    "check-github-tag": "gulp check-github-tag",
    "upload-screenshots": "gulp upload-screenshots",
    "s3-cleanup": "gulp s3-cleanup",
    "deployment-master": "gulp deployment-master",
    "deployment-devel": "gulp deployment-devel",
    "deployment-branch": "gulp deployment-branch",
    "git-config": "git config --global user.email \"ci@syncano.com\" && git config --global user.name \"CI\"",
    "predeployment-master": "npm version patch -m \"Release v%s [ci skip]\"",
    "postversion": "git push origin master && git push origin --tags"
