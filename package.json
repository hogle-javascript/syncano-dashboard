{
  "name": "syncano-dashboard",
  "version": "0.0.1",
  "description": "Syncano Dashboard",
  "main": "js/app.jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/Syncano/syncano-dashboard.git"
  },
  "dependencies": {
    "brace": "^0.5.1",
    "classnames": "^2.1.2",
    "gravatar": "^1.1.1",
    "hellojs": "^1.6.0",
    "keymaster": "~1.6.2",
    "material-ui": "Syncano/material-ui#build",
    "moment": "^2.10.3",
    "node-uuid": "^1.4.3",
    "normalize.css": "^3.0.3",
    "object-assign": "^3.0.0",
    "raven-js": "^1.1.19",
    "radium": "0.13.0",
    "react": "0.13.3",
    "react-onclickoutside": "^0.2.4",
    "react-outsideclickhandler": "latest",
    "react-router": "^0.13.3",
    "react-tap-event-plugin": "^0.1.7",
    "react-zeroclipboard": "^2.0.1",
    "reflux": "latest",
    "request": "^2.58.0",
    "superagent": "^1.2.0",
    "validate.js": "^0.7.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "aws-sdk": "*",
    "babel-core": "^5.6.15",
    "babel-loader": "^5.2.2",
    "css-loader": "^0.15.1",
    "del": "^1.2.0",
    "file-loader": "^0.8.4",
    "gulp": "^3.9.0",
    "gulp-awspublish": "^2.0.2",
    "gulp-chmod": "^1.2.0",
    "gulp-cloudfront": "0.0.14",
    "gulp-download": "0.0.1",
    "gulp-iconfont": "~3.0.2",
    "gulp-iconfont-css": "0.0.9",
    "gulp-rev": "^5.0.1",
    "gulp-rev-replace": "^0.4.2",
    "gulp-strip-debug": "^1.0.2",
    "gulp-unzip": "^0.1.3",
    "gulp-util": "^3.0.6",
    "jest-cli": "^0.4.14",
    "nightwatch": "^0.7.5",
    "node-libcompass": "git://github.com/patrykkopycinski/node-libcompass.git",
    "node-sass": "~3.2.0",
    "q": "^1.4.1",
    "raw-loader": "^0.5.1",
    "react-inlinesvg": "patrykkopycinski/react-inlinesvg#build",
    "sass-loader": "~1.0.2",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.0",
    "webpack-dev-server": "^1.10.1",
    "esprima-fb": "^15001.1.0-dev-harmony-fb",
    "jscs": "latest",
    "codeclimate-test-reporter": "0.1.0"
  },
  "jest": {
    "rootDir": ".",
    "testPathDirs": [
      "src/common",
      "src/apps"
    ],
    "testPathIgnorePatterns": [
      "/__tests__/[^/]*/.+"
    ],
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "collectCoverage": "true",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "css",
      "svg"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/classnames",
      "<rootDir>/src/utils/utils",
      "<rootDir>/src/utils/ReactRouterContext",
      "<rootDir>/src/utils/StorageMock",
      "<rootDir>/node_modules/superagent",
      "<rootDir>/node_modules/object-assign",
      "<rootDir>/node_modules/classnames",
      "<rootDir>material-ui",
      "<rootDir>/node_modules/hellojs"
    ]
  },
  "scripts": {
    "test": "jest -w 3",
    "start": "gulp webpack-dev-server",
    "build": "gulp build",
    "publish": "gulp publish",
    "clean": "gulp clean",
    "nightwatch-setup": "gulp nightwatch-setup:linux64",
    "nightwatch": "nightwatch",
    "jscs": "jscs --esprima=esprima-fb src",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  }
}
