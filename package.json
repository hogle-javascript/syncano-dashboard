{
  "name": "syncano-gui",
  "version": "0.0.1",
  "description": "Syncano GUI",
  "main": "js/app.jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/Syncano/syncano-gui.git"
  },
  "dependencies": {
    "brace": "^0.5.0",
    "classnames": "^1.2.0",
    "director": "^1.2.7",
    "flux": "^2.0.0",
    "gravatar": "^1.1.0",
    "hellojs": "^1.5.0",
    "keymaster": "~1.6.2",
    "moment": "^2.9.0",
    "node-uuid": "^1.4.3",
    "object-assign": "^2.0.0",
    "react": "latest",
    "react-onclickoutside": "^0.2.4",
    "superagent": "^0.21.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^1.0.0",
    "css-loader": "^0.9.0",
    "file-loader": "^0.8.1",
    "jest-cli": "^0.4.0",
    "jsx-loader": "^0.12.2",
    "raw-loader": "^0.5.1",
    "react-tools": "latest",
    "style-loader": "^0.8.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.4.13",
    "webpack-dev-server": "^1.6.6",
    "aws-sdk": "*",
    "q": "*"
  },
  "jest": {
    "rootDir": ".",
    "testPathDirs": [
      "src/js/"
    ],
    "testPathIgnorePatterns": [
      "/__tests__/[^/]*/.+"
    ],
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "collectCoverage": "true",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "css",
      "svg"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/superagent",
      "<rootDir>/node_modules/object-assign"
    ]
  },
  "scripts": {
    "start": "npm run dev | npm run watch",
    "serve": "./node_modules/.bin/http-server -p 8888 -s",
    "watch": "DEVEL=true webpack -w",
    "dev": "webpack-dev-server --content-base public/ --https --port 8080"
  }
}
